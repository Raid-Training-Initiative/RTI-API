{
    "name": "rti-api",
    "version": "1.0.0",
    "description": "A REST api for RTI data.",
    "main": "dist/App.js",
    "dependencies": {
        "async-lock": "^1.4.1",
        "axios": "^1.7.7",
        "cors": "^2.8.5",
        "date-fns": "^3.6.0",
        "discord.js": "^14.16.1",
        "escape-string-regexp": "^4.0.0",
        "express": "^4.19.2",
        "jsonschema": "^1.4.1",
        "mongoose": "^7.8.1"
    },
    "devDependencies": {
        "@eslint/compat": "^1.1.1",
        "@eslint/js": "^9.9.1",
        "@types/async-lock": "^1.4.2",
        "@types/eslint__js": "^8.42.3",
        "@types/express": "^4.17.21",
        "@types/node": "^20.16.4",
        "eslint": "^9.9.1",
        "eslint-config-prettier": "^9.1.0",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "tsc-alias": "^1.8.10",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.19.0",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.4.0"
    },
    "scripts": {
        "tsc": "tsc && tsc-alias",
        "ts-node": "tsx src/App.ts",
        "dev": "nodemon --inspect=0.0.0.0:9229 src/App.ts",
